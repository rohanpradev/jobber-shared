{
  "name": "jobber-shared",
  "version": "1.0.2",
  "license": "MIT",
  "description": "Helpers library for Jobber app",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/jobber-shared.d.ts",
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "pnpm eslint src --ext js,ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "pnpm build && node ./dist/index.js",
    "api-report": "api-extractor run",
    "api-report-local": "api-extractor run --local",
    "api-docs": "api-documenter markdown -i temp -o docs",
    "build-with-docs": "pnpm build && pnpm api-report-local && pnpm api-docs",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "node",
    "pino",
    "express",
    "middleware"
  ],
  "author": "Rohan Pradev",
  "volta": {
    "node": "20.11.0",
    "pnpm": "8.14.1"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    }
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/rohanpradev/jobber-shared"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@microsoft/api-documenter": "^7.23.17",
    "@microsoft/api-extractor": "^7.39.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "cloudinary": "^1.41.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "pino": "^8.17.2",
    "pino-elasticsearch": "^8.0.0"
  }
}
